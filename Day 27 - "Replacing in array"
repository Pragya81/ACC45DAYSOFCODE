#include<iostream>
#include<cstring>
using namespace std;
void replaceCharacter(char input[], char c1, char c2) 
{
    int length = strlen(input);
    for(int i = 0; i<length; i++)
    {
        if(input[i] == c1)
        {
           input[i] == c2; 
        }
    }
    for (int i = 0; i < length; i++)
    {
        cout << input[i] << endl;
    }
    
}
int main()
{
    char input[5], c1, c2;
    cin >> input;
    cin >> c1 >> c2;
    replaceCharacter(input[], c1, c2);
    
    return 0;
}
