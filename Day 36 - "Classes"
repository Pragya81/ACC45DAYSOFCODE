class Student:
    def __init__(self, name, age, id):
        self.name = name
        self.age = age;
        self.id = id;
    def marks(self, m1, m2):
        self.m1 = m1;
        self.m2 = m2;
    def display(self):
        print("Name:", self.name , "Age:", self.age , "ID:", id);
        
    
s = Student("Asta", 12, "12") 
s.display() 
