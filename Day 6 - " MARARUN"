#include <iostream>
using namespace std;

int main() {
	int t, D, d, a, b, c, x;
	cin >> t;
	while(t--){
	    cin >> D >> d >> a >> b >> c;
	    x = D*d;
	    if(x >= 10 && x < 21)     cout << a << endl;
	    else if(x >= 21 && x <42) cout << b << endl;
	    else if(x >= 42)          cout << c << endl;
	    else                      cout << "0" << endl;
	}
	return 0;
}
