#include<iostream>
using namespace std;

void arrange(int *arr, int n)
{
    int num = 1, m = 1;
    while(num <= n)
    {
        cout << m << ' ';
        m+=2;
        num++;
    }
    int n1=n-2;
    while(num <= n)
    {
        cout << n1 << ' ';
        n1-=2;
        n--;
    }
}
int main()
{
    int n;
    cin >> n;
    int arr[n];
    arrange(arr, n);
    
    return 0;
}
