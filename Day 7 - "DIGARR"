#include <bits/stdc++.h>
using namespace std;
int main() {
	int t;std::cin >> t;
	while(t--){
	    int d;cin>>d;
	    string s; 
	    cin>>s;
	    string ans="No";
	    
	    for(int i=0 ; i<s.length();i++){
	        if(s[i]=='0' || s[i]=='5'){   ans="Yes";break;
	        }
	    }
	    cout<<ans<<" ";
	}
	return 0;
}
